package com.cg.program1;

import java.util.Comparator;

public class salComp implements Comparator<Employees> {

    @Override
    public int compare(Employees o1, Employees o2) {
        if (o1.getSal() > o2.getSal()) {
            return 1;
        } else {
            return -1;
        }

    }

}
