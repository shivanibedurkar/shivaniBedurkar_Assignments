package com.cg.program3;

public class Program3 {

	public static void main(String[] args) {
		
		Account account[]=new Account[4];
		
		account[0]  = new SavingAccount(101,"Pallavi",50000);
		account[1]= new SavingAccount(102,"Priti",70000);
		account[2] = new CurrentAccount(201, "Mona", 50000);
		account[3] = new CurrentAccount(202, "Neha", 100000);
		
		int totalCash=0;
		
		for (Account account2 : account) {
			
			totalCash+=account2.getBalance();
			System.out.println("Account Holder:" +account2.getAccountHolder()+"----"+"Balance:"+ account2.getBalance());
		}
		System.out.println("Total cash in bank: "+totalCash);


	}

}
